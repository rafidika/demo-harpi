verified) {\n            setSignedIn(true);\n            console.log(\"Success!\");\n        }\n    }, []);\n\n    return (\n        <Fragment>\n            {signedIn ? <Main onRouteChange={onRouteChange} /> : <SignIn onRouteChange={onRouteChange} />\n            }\n        </Fragment>\n    )\n}\n\nexport default Admin;","/Users/rafidika/Documents/Project/Harpi Melati/FirstSimpleWebsite/client/src/components/Form.js",[],"/Users/rafidika/Documents/Project/Harpi Melati/FirstSimpleWebsite/client/src/components/SignIn.js",["78","79"],"import React, { useState, useEffect } from 'react'\nimport Axios from 'axios'\nimport '../styles/adminloginXL.css'\nimport Alert from './Alert';\n\nexport default function SignIn( {onRouteChange} ) {\n    const [success, setSuccess] = useState(\"init\");\n    const [Username, setUsername] = useState(\"\");\n    const [Password, setPassword] = useState(\"\");\n    const [alert, setAlert] = useState({show: false, msg: \"\", type: \"\"})\n\n    const signIn = async (e) => {\n        e.preventDefault();\n        try {\n            const signInData = new FormData();\n            signInData.append(\"Username\", Username);\n            signInData.append(\"Password\", Password);\n\n            await Axios.post(\"http://localhost:8080/signin\", signInData, {\n                headers: {\n                    \"Content-Type\":\"multipart/form-data\"\n                }\n            })\n            .then(res => {\n                if (res) {\n                    setSuccess(\"init\");\n                    localStorage.setItem(\"token\", res.data.accessToken);\n                    onRouteChange(true);\n                } else {\n                    showAlert(true, res.data, \"danger\")\n                }\n            })\n        } catch (err) {\n            console.error(err.message);\n        }\n    }\n\n    const showAlert = (show = false, msg = \"\", type=\"\") => {\n        setAlert({show, msg, type})\n    }\n\n    return (\n        <div className=\"container-fluid\">\n            <div className=\"row min-vw-100 min-vh-100\">\n                {/* Green thingy */}\n                <div className=\"col-xl-7\" id=\"rest\">\n                    <div className=\"row\">\n                        <div className=\"col-3 col-xl-3\" id=\"logo\">\n                            <img src=\"assets/logo-transparent.png\" className=\"img-fluid logo-main-page\" alt=\"Logo HARPI MELATI\"/>\n                        </div>\n                        <div className=\"col-9 col-xl-9\" id=\"himpunan\">\n                            <h3 className=\"huge-text\">HARPI MELATI</h3>\n                            <p className=\"huge-text\">Himpunan Perias Pengantin Indonesia \"MELATI\"</p>\n                        </div>\n                    </div>\n                </div>\n                {/* Form */}\n                <div className=\"col-xl-5 d-flex justify-content-center align-items-center\" id=\"form\" >\n                    <form onSubmit={signIn}>\n                        <p id=\"title\">Masuk ke Halaman Admin</p>\n                        <br /><br />\n                        {alert.show && <Alert msg={alert.msg} type={alert.type} removeAlert={showAlert}/>}\n                        <label htmlFor=\"username\">Username</label>\n                        <br />\n                        <input type=\"text\" name=\"Username\" required id=\"username\" value={Username} onChange={e => setUsername(e.target.value)}/>\n                        <br /><br />\n                        <label htmlFor=\"password\">Password</label>\n                        <br />\n                        <input type=\"password\" name=\"Password\" required id=\"password\" value={Password} onChange={e => setPassword(e.target.value)}/>\n                        <br /><br />\n                        <input type=\"submit\" value=\"Masuk\" id=\"masuk\"/>\n                        <br /><br />\n                        <p id=\"copyright\">Copyright 2021 Himpunan Perias Pengantin Indonesia \"MELATI\"</p>\n                    </form>\n                </div>\n            </div>\n        </div>\n    )\n}\n","/Users/rafidika/Documents/Project/Harpi Melati/FirstSimpleWebsite/client/src/components/Alert.js",["80","81"],"import React, { useEffect } from 'react'\n\nexport default function Alert({msg, type, removeAlert}) {\n    useEffect(() => {\n        const timeout = setTimeout(() => {\n            removeAlert()\n        }, 3000);\n        return () => clearTimeout;\n    }, []);\n    \n    return (\n        <div>\n           <p className={`alert alert-${type}`}>{msg}</p> \n        </div>\n    )\n}\n","/Users/rafidika/Documents/Project/Harpi Melati/FirstSimpleWebsite/client/src/components/Main.js",[],"/Users/rafidika/Documents/Project/Harpi Melati/FirstSimpleWebsite/client/src/components/MemberData.js",[],"/Users/rafidika/Documents/Project/Harpi Melati/FirstSimpleWebsite/client/src/components/NewMembers.js",[],"/Users/rafidika/Documents/Project/Harpi Melati/FirstSimpleWebsite/client/src/components/Navbar.js",["82","83"],"import React from 'react'\nimport '../styles/admin.css'\n\nexport default function Navbar({ onRouteChange }) {\n    const signOut = () => {\n        localStorage.removeItem(\"token\");\n        onRouteChange(false);\n    }\n\n    return (\n        <nav class=\"navbar navbar-expand-lg navbar-light sticky-top\">\n            <div class=\"container-fluid\">\n                <a class=\"navbar-brand\" href=\"#\">\n                    <img src=\"assets/logo-transparent.png\" alt=\"Logo HARPI MELATI\" className=\"logo-navbar\"/>\n                </a>\n                <button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarSupportedContent\" aria-controls=\"navbarSupportedContent\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n                    <span class=\"navbar-toggler-icon\"></span>\n                </button>\n                <span class=\"navbar-text\" id=\"text\">\n                    